<h1 class="text-center"><%= @challenge.name %></h1>

<p class="text-justify"> <%= @challenge.content %> </p>
<div class="bg-info text-black text-decoration-none text-center ">
<%= link_to "Go!", challenge_dares_path(@challenge), class: "bg-info text-black text-decoration-none", method: :post %>
</div>

<div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>